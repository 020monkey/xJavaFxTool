<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="435.0" prefWidth="780.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.xwintop.xJavaFxTool.controller.littleTools.FileCopyController">
   <children>
      <Label layoutX="14.0" layoutY="11.0" text="原文件路径：" />
      <TextField fx:id="textFieldCopyFileOriginalPath" layoutX="79.0" layoutY="7.0" prefHeight="23.0" prefWidth="95.0" />
      <Label layoutX="228.0" layoutY="11.0" text="拷贝路径：" />
      <TextField fx:id="textFieldCopyFileTargetPath" layoutX="283.0" layoutY="8.0" prefHeight="23.0" prefWidth="102.0" />
      <CheckBox fx:id="checkBoxIsCopy" layoutX="563.0" layoutY="12.0" mnemonicParsing="false" selected="true" text="拷贝" />
      <Button fx:id="buttonCopy" layoutX="706.0" layoutY="368.0" mnemonicParsing="false" onAction="#copyAction" text="一键拷贝" />
      <Spinner fx:id="spinnerCopyNumber" layoutX="486.0" layoutY="9.0" prefHeight="23.0" prefWidth="64.0" />
      <Label layoutX="432.0" layoutY="13.0" text="拷贝份数" />
      <TableView fx:id="tableViewMain" editable="true" layoutX="13.0" layoutY="44.0" prefHeight="314.0" prefWidth="759.0">
        <columns>
          <TableColumn fx:id="tableColumnCopyFileOriginalPath" prefWidth="281.0" sortable="false" text="原文件路径" />
          <TableColumn fx:id="tableColumnCopyFileTargetPath" prefWidth="249.0" sortable="false" text="拷贝路径" />
            <TableColumn fx:id="tableColumnCopyNumber" prefWidth="59.0" sortable="false" text="拷贝份数" />
            <TableColumn fx:id="tableColumnIsCopy" prefWidth="57.0" sortable="false" text="是否拷贝" />
            <TableColumn fx:id="tableColumnIsRename" minWidth="0.0" prefWidth="44.0" sortable="false" text="重命名" />
            <TableColumn fx:id="tableColumnIsDelete" prefWidth="68.0" sortable="false" text="删除原文件" />
        </columns>
      </TableView>
      <CheckBox fx:id="checkBoxIsDelete" layoutX="617.0" layoutY="13.0" mnemonicParsing="false" text="删除" />
      <Button fx:id="buttonAddItem" layoutX="732.0" layoutY="9.0" mnemonicParsing="false" onAction="#addItemAction" text="添加" />
      <Button fx:id="buttonSaveConfigure" layoutX="13.0" layoutY="368.0" mnemonicParsing="false" onAction="#saveConfigure" text="保存配置" />
      <Button fx:id="buttonChooseOriginalPath" layoutX="175.0" layoutY="7.0" mnemonicParsing="false" onAction="#chooseOriginalPathAction" prefHeight="23.0" prefWidth="40.0" text="选择" />
      <Button fx:id="buttonChooseTargetPath" layoutX="385.0" layoutY="8.0" mnemonicParsing="false" onAction="#chooseTargetPathAction" text="选择" />
      <Button fx:id="buttonDeleteSelectRow" layoutX="228.0" layoutY="368.0" mnemonicParsing="false" onAction="#deleteSelectRowAction" text="删除选中行" />
      <Button fx:id="otherSaveConfigureButton" layoutX="79.0" layoutY="368.0" mnemonicParsing="false" onAction="#otherSaveConfigureAction" text="配置另存为" />
      <Button fx:id="loadingConfigureButton" layoutX="158.0" layoutY="368.0" mnemonicParsing="false" onAction="#loadingConfigureAction" text="加载配置" />
      <Button fx:id="runQuartzButton" layoutX="630.0" layoutY="368.0" mnemonicParsing="false" onAction="#runQuartzAction" text="定时运行" />
      <TextField fx:id="cronTextField" layoutX="420.0" layoutY="368.0" prefHeight="23.0" prefWidth="188.0" visible="false" />
      <ChoiceBox fx:id="quartzChoiceBox" layoutX="309.0" layoutY="368.0" prefHeight="23.0" prefWidth="102.0" />
      <AnchorPane fx:id="simpleScheduleAnchorPane" layoutX="420.0" layoutY="367.0" prefHeight="23.0" prefWidth="200.0">
         <children>
            <Label layoutX="2.0" layoutY="5.0" text="间隔" />
            <Spinner fx:id="intervalSpinner" editable="true" layoutX="30.0" prefHeight="23.0" prefWidth="66.0">
               <tooltip>
                  <Tooltip text="单位为秒" />
               </tooltip></Spinner>
            <Label layoutX="103.0" layoutY="4.0" text="次数" />
            <Spinner fx:id="repeatCountSpinner" editable="true" layoutX="131.0" prefHeight="23.0" prefWidth="64.0">
               <tooltip>
                  <Tooltip text="-1为永久任务" />
               </tooltip></Spinner>
         </children>
      </AnchorPane>
      <CheckBox fx:id="checkBoxIsRename" layoutX="671.0" layoutY="13.0" mnemonicParsing="false" text="重命名">
         <tooltip>
            <Tooltip text="在名称后加随机数防止文件重复" />
         </tooltip></CheckBox>
   </children>
</AnchorPane>
